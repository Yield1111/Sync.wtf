while wait(0.05) do
-- Define the target position
local targetPosition = Vector3.new(-19.1936417, 5.40206289, -198.450699)

-- Loop through all models under workspace.Bags
for _, model in ipairs(workspace.Bags:GetChildren()) do
    -- Check if the child is a Model
    if model:IsA("Model") then
        -- Look for a part named "MoneyBag" inside the model
        local moneyBag = model:FindFirstChild("MoneyBag")
        
        -- If the MoneyBag part is found, teleport it
        if moneyBag and moneyBag:IsA("BasePart") then
            moneyBag.CFrame = CFrame.new(targetPosition)
            print("Teleported " .. model.Name .. "'s MoneyBag successfully!")
        else
            warn("No MoneyBag part found in model: " .. model.Name)
        end
    else
        warn(model.Name .. " is not a Model and was skipped.")
    end
end

end
