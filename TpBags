-- Define the target position
local targetPosition = Vector3.new(-19.1936417, 5.40206289, -198.450699)

-- Loop through all children under workspace.Bags
for _, child in ipairs(workspace.Bags:GetChildren()) do
    -- Check if the child is a BasePart (e.g., part, mesh, etc.)
    if child:IsA("BasePart") then
        -- Teleport the part to the target position
        child.CFrame = CFrame.new(targetPosition)
        print(child.Name .. " teleported successfully!")
    else
        warn(child.Name .. " is not a BasePart and was skipped.")
    end
end
