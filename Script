local P=game:GetService("Players") local p=P.LocalPlayer local g=p:WaitForChild("PlayerGui") local s=Instance.new("ScreenGui") s.Parent=g local b=Instance.new("TextButton") b.Size=UDim2.new(0,200,0,50) b.Position=UDim2.new(0.5,-100,0.5,-25) b.Text="Load Sync.wtf" b.BackgroundColor3=Color3.new(0.172549,0.039216,0.039216) b.TextColor3=Color3.new(0,0,0) b.Parent=s local c=Instance.new("UICorner") c.CornerRadius=UDim.new(0,12) c.Parent=b b.MouseButton1Click:Connect(function() local t=game:GetService("TweenService") local r=t:Create(b,TweenInfo.new(0.3,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{Size=UDim2.new(0,150,0,40),Position=UDim2.new(0.5,-75,0.5,-20),TextTransparency=1}) r:Play() r.Completed:Wait() b.Visible=false local u="https://raw.githubusercontent.com/Yield1111/Sync.wtf/refs/heads/main/gdfd" local f,m=pcall(function() loadstring(game:HttpGet(u))() end) if not f then warn("Error loading script: "..m) end end)
